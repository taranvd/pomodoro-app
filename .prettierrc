{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "importOrder": [
    "<THIRD_PARTY_MODULES",
    "^@/components/(.*)$",
    "^@/hoooks/(.*)$",
    "^@/services/(.*)$",
    "^@/assets/(.*)$",
    "^@/types/(.*)$",
    "^@/providers/(.*)$",
    "^../(.*)$",
    "^./(.*)$"
  ],
  "importOrdedSeparation": true,
  "importOrderSortSpecifiers": true
}
